{
    "name": "railken/lara-ore-address",
    "keywords": ["api"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/bag": "^1.2",
        "railken/laravel-manager": "^3.1",
        "railken/lara-ore-api": "dev-master",
        "railken/lara-ore-support": "dev-master",
        "railken/lara-ore-user": "dev-master",
        "ronanguilloux/isocodes": "^2.1",
        "league/iso3166": "^2.1",
        "fzaninotto/faker": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpstan/phpstan": "^0.9",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\LaraOre\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\LaraOre\\Address\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\LaraOre\\AddressServiceProvider"
            ]
        }
    }
}
