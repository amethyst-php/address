{
    "name": "amethyst/address",
    "keywords": ["api"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*",
        "amethyst/geolocation": "0.1.*",
        "ronanguilloux/isocodes": "^2.1",
        "league/iso3166": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
