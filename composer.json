{
    "name": "railken/amethyst-address",
    "keywords": ["api"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "ronanguilloux/isocodes": "^2.1",
        "league/iso3166": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpstan/phpstan": "^0.10",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\AddressServiceProvider"
            ]
        }
    }
}
